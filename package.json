    "esbuild": "^0.20.0",
    "typescript": "^5.3.0",
    "wrangler": "^3.28.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "url": "https://github.com/your-username/greeting-image-generator"

{
  "name": "greeting-image-generator",
  "version": "0.0.1",
  "description": "A greeting image generator running on Cloudflare Workers",
  "main": "src/index.ts",
  "scripts": {
    "build": "esbuild src/index.ts --bundle --outfile=dist/index.js --format=esm --target=es2022",
    "dev": "wrangler dev",
